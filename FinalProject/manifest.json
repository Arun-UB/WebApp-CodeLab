{
  "name": "wReader",
  "version": "0.0.2",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "app": {
    "background": {
      "scripts": ["js/background.js"]
    }
  },
  "permissions": [
    "experimental",
    "*://blog.chromium.org/feeds/*"
  ],
  "intents": {
    "http://webintents.org/view": [{ 
      "type": ["application/rss+xml", "application/atom+xml"],
      "title": "wReader"
    }]
  }
}
